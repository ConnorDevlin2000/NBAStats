#set( $title = "Show players" )
<body>
    #if ($players)
    <h1>List of all players:</h1>
    <div>
        <ol>
        <p>
            #foreach($p in $players)
                <li>
                    <i>$p.playerName</i>

                </li>
                <li>
                    <i>$p.playerID</i>
                </li>
                <li>
                    <i>$p.team.nickname</i>
                </li>
                <li>
                    <i>$p.team.teamID</i>
                </li>
                <li>
                    <i>$p.season</i>
                </li>
            #end
            </p>

        </ol>
    </div>
    #end
</body>